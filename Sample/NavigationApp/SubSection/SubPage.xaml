<wpf:BasePage
    x:Class="NavigationApp.SubSection.SubPage"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:local="clr-namespace:NavigationApp.SubSection"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:wpf="clr-namespace:MvvmToolkit.Navigation.WPF;assembly=MvvmToolkit.Navigation.Wpf"
    Title="MainPage"
    d:DesignHeight="450"
    d:DesignWidth="800"
    mc:Ignorable="d">

    <Grid>
        <TextBlock Text="Hej" />

        <Button
            Margin="20,20,0,0"
            HorizontalAlignment="Left"
            VerticalAlignment="Top"
            Command="{Binding GoBackCommand}"
            Content="Go back" />
    </Grid>
</wpf:BasePage>
